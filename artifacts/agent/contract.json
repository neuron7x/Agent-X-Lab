{
  "artifact_uploads": [
    {
      "missing_behavior": "error",
      "must_exist": true,
      "path": "artifacts/reports/**",
      "producer_step": "make proof",
      "workflow": "ci.yml"
    },
    {
      "missing_behavior": "error",
      "must_exist": true,
      "path": "objects/*/artifacts/evidence/reference/eval/report.json",
      "producer_step": "make check (evals)",
      "workflow": "ci.yml"
    },
    {
      "missing_behavior": "error",
      "must_exist": true,
      "path": "artifacts/security/pip-audit.json",
      "producer_step": "Initialize pip-audit report artifact + make vuln-scan",
      "workflow": "security.yml"
    },
    {
      "missing_behavior": "error",
      "must_exist": true,
      "path": "artifacts/security/scorecard-results.sarif",
      "producer_step": "ossf/scorecard-action",
      "workflow": "scorecard.yml"
    }
  ],
  "default_branch": "work",
  "local_entrypoints": [
    {
      "command": "make setup",
      "name": "setup"
    },
    {
      "command": "make check",
      "name": "check"
    },
    {
      "command": "make proof",
      "name": "proof"
    },
    {
      "command": "make vuln-scan",
      "name": "vuln-scan"
    },
    {
      "command": "make workflow-hygiene",
      "name": "workflow-hygiene"
    },
    {
      "command": "make action-pinning",
      "name": "action-pinning"
    }
  ],
  "required_checks": [
    {
      "command": "make setup && make check && python tools/feg_r8_verify.py && make proof",
      "name": "CI/quality",
      "required_artifacts": [
        "artifacts/reports/**",
        "objects/*/artifacts/evidence/reference/eval/report.json"
      ],
      "source": "workflow"
    },
    {
      "command": "test \"${needs.quality.result}\" = \"success\"",
      "name": "CI/status-check",
      "required_artifacts": [],
      "source": "workflow"
    },
    {
      "command": "make setup && python -m pip install pip-audit==2.9.0 && make vuln-scan",
      "name": "Security/pip-audit",
      "required_artifacts": [
        "artifacts/security/pip-audit.json"
      ],
      "source": "workflow"
    },
    {
      "command": "reviewdog/action-actionlint@v1",
      "name": "Lint GitHub Actions/actionlint",
      "required_artifacts": [],
      "source": "workflow"
    }
  ],
  "validators": [
    {
      "classification": "stable",
      "enforced_by": "scripts/validate_arsenal.py",
      "id": "arsenal-strict",
      "rationale": "checks tracked repo inputs and policies under strict mode",
      "requires_paths": [
        "MANIFEST.json",
        "catalog/**",
        "objects/**",
        "protocols/**"
      ]
    },
    {
      "classification": "transient",
      "enforced_by": "tools/titan9_inventory.py",
      "id": "titan9-inventory",
      "rationale": "generated at runtime and consumed by README contract verifier in same run",
      "requires_paths": [
        "artifacts/titan9/inventory.json"
      ]
    },
    {
      "classification": "transient",
      "enforced_by": "tools/pip_audit_gate.py",
      "id": "pip-audit-gate",
      "rationale": "security report artifact is CI upload target; script writes deterministic envelope on error",
      "requires_paths": [
        "artifacts/security/pip-audit.json",
        "policies/pip_audit_allowlist.json"
      ]
    }
  ]
}
